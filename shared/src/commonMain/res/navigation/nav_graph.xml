<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/archiveFragment">

    <fragment
        android:id="@+id/archiveFragment"
        android:name="com.misenpai.anivault.ui.home.seasonlayouts.Archive"
        tools:layout="@layout/fragment_archive" >
        <action
            android:id="@+id/action_archiveFragment_to_archiveSelectedFragment"
            app:destination="@id/archiveSelectedFragment" />
    </fragment>

    <fragment
        android:id="@+id/archiveSelectedFragment"
        android:name="com.misenpai.anivault.ui.home.seasonlayouts.ArchiveSelected"
        tools:layout="@layout/fragment_archive_selected" >
        <argument
            android:name="year"
            app:argType="integer" />
        <argument
            android:name="season"
            app:argType="string" />
    </fragment>
</navigation>
